# .zsh_alias

alias cdr=cd .
alias sourcez="source $HOME/.zshrc"

# ls aliases
alias la='ls -AF'
alias ll='ls -l'
alias l='ls -AlF'

# emacs aliases
alias emacs='/Applications/Emacs.app/Contents/MacOS/Emacs-arm64-11 -nw'
alias emasc='/Applications/Emacs.app/Contents/MacOS/Emacs-arm64-11 -nw'
alias emac='/Applications/Emacs.app/Contents/MacOS/Emacs-arm64-11 -nw'
