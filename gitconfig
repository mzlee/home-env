[user]
	name = Michael Lee
	email = mzlee@cs.utexas.edu
[alias]
	co = checkout
	ci = commit
	st = status
	ch = diff --name-status -r
	br = branch
	br-show = branch -a
	stash-diff = stash show --pretty=raw
	hist = log --date=short --graph --pretty=\"%h %ar | %s%d [%an]\"
	hist-stat = log --stat --graph --pretty=\"%h %ar |%C(yellow)%d%Creset%n%s [%an]\"
	type = cat-file -t
	dump = cat-file -p
	flash = !git stash && git pull --rebase ${@} && git stash pop && echo "Flashed "
[core]
	excludesfile = ~/.gitignore
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = always
	grep = always
[color "branch"]
	local = yellow
	remote = green
	current = yellow reverse
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	conflictstyle = diff3
[push]
	default = current
