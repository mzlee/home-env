[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	local = yellow
	remote = green
	current = yellow reverse
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
